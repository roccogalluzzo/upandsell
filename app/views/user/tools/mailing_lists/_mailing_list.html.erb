<tr id="js-ml-<%= mailing_list.id %>">
  <td>
    <% if !mailing_list.mailchimp_list_id.blank? %>
    <%= image_tag "mailchimp_logo.png",
    title: "Synced with: #{mailing_list.mailchimp_list_name}",
    class: 'mailchimp-list-logo', width: '20px' %>
    <% else %>
    <%= image_tag "mailchimp_logo_greyed.png",
    class: 'mailchimp-list-logo', width: '20px' %>
    <% end %>
    <% if !mailing_list.createsend_list_id.blank? %>
    <%= image_tag "campaign_monitor_logo.png",
    title: "Synced with: #{mailing_list.createsend_list_name}",
    class: 'payment-logo', height: '20px' %>
    <% else %>
    <%= image_tag "campaign_monitor_logo_greyed.png",
    class: 'payment-logo', height: '20px' %>
    <% end %>
  </td>
  <td> <%= mailing_list.name %> </td>
  <td> <%= mailing_list.subscribers %> </td>
  <td> <i class="text-muted"><%= mailing_list.last_sent || 'Never' %></i></td>
  <td>
   <%= link_to user_tools_mailing_list_path(mailing_list),
   remote: true, method: :delete, class: 'btn-circle' do %>
   <i class="fa fa-trash-o"></i>
   <% end %>

   <%= link_to new_user_tools_mailing_list_emails_path(mailing_list), class: 'btn-send' do %>
   <i class="fa fa-send"></i>
   <% end %>
 </td>
</tr>